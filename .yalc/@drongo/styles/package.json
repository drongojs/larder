{
  "name": "@drongo/styles",
  "version": "0.0.0-84f6c88b",
  "main": "dist/styler.cjs.js",
  "module": "dist/styler.es.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:drongojs/styler.git",
  "author": "Jack Ellis <jack.ellis@godaddy.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:clean && yarn build:js && yarn build:ts && yarn build:post",
    "build:clean": "rimraf dist",
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "build:ts": "tsc --outDir dist --declarationDir dist --declaration --emitDeclarationOnly --noEmit false",
    "build:post": "rimraf dist/**/__tests__",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint src --ext ts",
    "lint:ts": "tsc",
    "prepublishOnly": "yarn build",
    "test": "cross-env NODE_ENV=test ava",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk node_modules/ava/profile",
    "test:coverage": "cross-env NODE_ENV=test nyc ava",
    "ci:prepare": "yarn install && yarn build && yarn lint && yarn flow && yarn test:coverage",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "aphrodite": "^2.3.1"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  }
}
