{
  "name": "@drongo/ux",
  "version": "0.0.0-cb229eb2",
  "main": "index.js",
  "repository": "git@github.com:drongojs/ux.git",
  "author": "Jack Ellis <jack.ellis@godaddy.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:clean && yarn build:js && yarn build:ts && yarn build:post",
    "build:clean": "echo \"noop\"",
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "build:ts": "#tsc --outDir . --declarationDir . --declaration --emitDeclarationOnly --noEmit false",
    "build:post": "rm index.js",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint src --ext ts",
    "lint:ts": "tsc",
    "prepublishOnly": "yarn build",
    "test": "#cross-env NODE_ENV=test ava",
    "test:watch": "yarn test --watch",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk node_modules/ava/profile",
    "test:coverage": "cross-env NODE_ENV=test nyc ava",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@drongo/styles": "*",
    "@vue/composition-api": "^0.x.x",
    "vue": "^2.6.10"
  },
  "dependencies": {
    "@types/color": "^3.0.0",
    "color": "^3.1.2",
    "font-awesome": "^4.7.0",
    "ulid": "^2.3.0",
    "vue-compose": "^0.7.3",
    "vue-hoc": "^0.4.7"
  }
}
