{
  "name": "@drongo/larder",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:drongojs/larder.git",
  "author": "Jack Ellis <jack.ellis@godaddy.com>",
  "license": "MIT",
  "scripts": {
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint src --ext ts,tsx",
    "lint:ts": "tsc",
    "test": "jest",
    "test:coverage": "STORY_MODE=BACKSTOP jest --coverage",
    "test:watch": "yarn test --watch",
    "test:integration": "NODE_ENV=test cucumber-js --require \"./feature-setup.js\" --require \"src/**/__integration__/**/*.steps.ts\"",
    "test:backstop": "backstop test --config backstop.config.js",
    "test:backstop:approve": "backstop approve --config backstop.config.js",
    "build": "rm -rf public && NODE_ENV=production webpack --progress --colors",
    "build:dev": "rm -rf public && NODE_ENV=development webpack --progress --colors",
    "start:dev": "webpack-dev-server",
    "storybook": "STORY_MODE=STORYBOOK start-storybook -p 6006 --no-dll",
    "storybook:integration": "STORY_MODE=INT start-storybook -p 6006 --no-dll",
    "storybook:backstop": "STORY_MODE=backstop start-storybook -p 6006 --no-dll",
    "build:storybook:integration": "STORY_MODE=INT build-storybook --no-dll"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.11.5",
    "@cucumber/cucumber": "^7.0.0-rc.0",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-storyshots": "^6.1.1",
    "@storybook/react": "^6.0.28",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/chai": "^4.2.13",
    "@types/color": "^3.0.1",
    "@types/convert-units": "^2.3.2",
    "@types/jest": "^26.0.15",
    "@types/long": "^4.0.1",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/selenium-webdriver": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-jsx-control-statements": "^4.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "backstopjs": "^5.0.6",
    "chai": "^4.2.0",
    "chromedriver": "^85.0.1",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react": "^7.20.6",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.11.0",
    "msw": "^0.21.2",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "typescript": "^3.8.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "color": "^3.1.3",
    "convert-units": "^2.3.4",
    "formik": "^2.1.5",
    "jpex": "^4.1.0",
    "linaria": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^2.3.1",
    "react-jpex": "^1.4.0",
    "react-query": "^2.15.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "use-debounce": "^4.0.0"
  }
}
