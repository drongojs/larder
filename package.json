{
  "name": "@drongo/larder",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:drongojs/larder.git",
  "author": "Jack Ellis <jack.ellis@godaddy.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "snowpack --clean",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint src --ext ts",
    "lint:ts": "tsc",
    "test": "ava",
    "test:watch": "yarn test --watch",
    "build": "yarn build:js && yarn build:html && yarn build:post",
    "build:watch": "yarn build && yarn build:js --watch --verbose",
    "build:js": "yarn babel -d \"lib/\" --delete-dir-on-start -x \".ts\",\".tsx\"  \"src/\"",
    "build:html": "cp src/presentation/index.html index.html",
    "build:post": "echo \"done\"",
    "build:rollup": "cross-env NODE_ENV=production rollup -c",
    "start:dev": "browser-refresh devserver"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.8.6",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "babel-preset-vue": "^2.0.2",
    "browser-refresh": "^1.7.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "express": "^4.17.1",
    "rollup": "^2.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.4.1",
    "rollup-plugin-vue": "^5.1.6",
    "snowpack": "1.5.x",
    "typescript": "^3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@drongo/styles": "file:.yalc/@drongo/styles",
    "@drongo/ux": "file:.yalc/@drongo/ux",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/composition-api": "^0.4.0",
    "vue": "^2.6.11",
    "vue-component-router": "^0.1.17"
  }
}
